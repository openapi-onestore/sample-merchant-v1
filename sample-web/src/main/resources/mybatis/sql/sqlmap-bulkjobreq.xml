<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="bulkJobRequest">
	<resultMap id="bulkJobRequestMap" type="map">
		<result property="mid" column="mid" />
		<result property="status" column="status" />
		<result property="reason" column="reason" />
		<result property="waitingJob" column="waiting_jobs" />
		<result property="jobId" column="job_id" />
		<result property="uploadFile" column="upload_file" />
		<result property="uploadDate" column="upload_date" />
	</resultMap>

	<select id="selectBulkJobRequest" parameterType="map" resultMap="bulkJobRequestMap">
		SELECT * FROM BULK_JOB_REQUEST
		WHERE MID BETWEEN #{START_NO} AND #{END_NO}
	</select>
</mapper>